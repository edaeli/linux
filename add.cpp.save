#include <iostream>
#include <cstdlib> // atoi

int main(int argc, char* argv[]) {
    if (argc != 3) {
        std::cerr << "Usage: " << argv[0] << " a b\n";
        return 1;
    }

    int a = std::atoi(argv[1]);
    int b = std::atoi(argv[2]);

    std::cout << a << " + " << b << " = " << (a + b) << "\n";
    return 0;
}

